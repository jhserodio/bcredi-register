{
  "name": "bcredi-register",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "history": "^4.9.0",
    "lodash.debounce": "^4.0.8",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-intl": "^2.9.0",
    "react-redux": "^7.1.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "3.0.1",
    "react-tooltip": "^3.10.0",
    "redux": "^4.0.1",
    "redux-starter-kit": "^0.5.1",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "precommit": "lint-staged",
    "eslint": "eslint -c .eslintrc --ignore-path .eslintignore '**/*.{js,jsx}' --quiet",
    "eslint:fix": "eslint -c .eslintrc --ext .js --ext .jsx --ignore-path .eslintignore --fix '**/*.{js,jsx}' --quiet",
    "stylelint": "stylelint src/**/*.css",
    "eject": "react-scripts eject",
    "cover": "nyc npm run test",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "commitizen": "^2.10.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.3.1",
    "lint-staged": "^7.3.0",
    "nock": "^10.0.0",
    "prettier": "1.14.3",
    "sinon": "^6.3.4",
    "standard-version": "^4.4.0",
    "stylelint": "^9.6.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-declaration-use-variable": "^1.7.0",
    "stylelint-prettier": "^1.0.1"
  },
  "nyc": {
    "include": [
      "src/**/*.js",
      "src/**/*.jsx"
    ],
    "exclude": [
      "**/*.test.js",
      "**/*.mock.js",
      "utils/flattenMessages.js",
      "utils/intlEnzyme.js",
      "redux/store.js",
      "redux/reducer.js",
      "redux/middleware.js",
      "src/messages.js",
      "src/containers"
    ],
    "reporter": [
      "lcov",
      "text",
      "html"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": [
      "yarn run stylelint"
    ],
    "*.{js,jsx}": [
      "yarn run eslint:fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
